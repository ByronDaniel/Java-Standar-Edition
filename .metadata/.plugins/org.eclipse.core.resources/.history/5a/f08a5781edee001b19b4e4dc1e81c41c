
public class TestAuto {

	public static void main(String[] args) {
		Auto auto1;
		auto1 = new Auto();
		auto1.marca = "FORD";
		auto1.anio = 2021;
		auto1.precio = 60000.10;
		System.out.println("el auto es "+auto1.marca+", del año "+auto1.anio+" y cuesta "+auto1.precio);
		
		Auto auto2;
		auto2 = new Auto();
		System.out.println(auto2.marca);
		
	}

}
